{
  "name": "nutri-bot",
  "version": "1.0.0",
  "description": "LLM-powered nutritionist assistant with Spanish privacy law compliance",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon src/server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node src/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "node src/database/migrate.js",
    "db:seed": "node src/database/seed.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "security:audit": "npm audit",
    "privacy:check": "node src/utils/privacy-compliance-check.js"
  },
  "keywords": [
    "llm",
    "chatbot",
    "nutritionist",
    "privacy",
    "gdpr",
    "lopdgdd",
    "spain",
    "health",
    "ai"
  ],
  "author": "NutriBot Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "sequelize": "^6.35.1",
    "pg-hstore": "^2.3.4",
    "openai": "^4.20.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "joi": "^17.11.0",
    "express-session": "^1.17.3",
    "connect-pg-simple": "^7.0.0",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "redis": "^4.6.10",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "qrcode": "^1.5.3",
    "speakeasy": "^2.0.0",
    "sanitize-html": "^2.11.0",
    "xss": "^1.0.14",
    "sql-injection": "^1.0.0",
    "express-http-proxy": "^1.6.3",
    "compression-webpack-plugin": "^10.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nutribot/nutri-bot.git"
  },
  "bugs": {
    "url": "https://github.com/nutribot/nutri-bot/issues"
  },
  "homepage": "https://github.com/nutribot/nutri-bot#readme"
}

